	<xs:complexType name="languageInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the languages represented in the resource </xs:documentation>
			<xs:appinfo>
				<render-short>{languageName} {languageVarietyInfo}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ms:languageId">
				<xs:annotation>
					<xs:documentation>The identifier of the language that is included in the resource or supported by the tool/service; an autocompletion mechanism with values from the ISO 639 is provided in the editor, but the values can be subsequently edited for further specification (according to the IETF BCP47 guidelines)</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ms:languageName">
