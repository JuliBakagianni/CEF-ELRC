	<xs:include schemaLocation="META-SHARE-RoleTypes.xsd"/>
	<xs:include schemaLocation="META-SHARE-LicenseMetadata.xsd"/>
	<!-- definition of complex types -->
	<!-- media type info TextInfo/imageInfo/AudioInfo/VideInfo etc-->
	<xs:complexType name="characterEncodingInfoType">
		<xs:annotation>
			<xs:documentation>Groups together information on character encoding of the
				resource</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ms:characterEncoding">
				<xs:annotation>
					<xs:documentation>The name of the character encoding used in the resource or
						accepted by the tool/service</xs:documentation>
					<xs:appinfo>
						<relation>one-to-many</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<!--
			<xs:element name="characterSet" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:appinfo>
						<relation>one-to-many</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			-->
			<xs:element name="sizePerCharacterEncoding" type="ms:sizeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides information on the size of the resource parts with
						different character encoding</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<inline-type>tabular</inline-type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="timeCoverageInfoType">
		<xs:annotation>
			<xs:documentation>Groups together information on time classification of the
				resource</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="timeCoverage">
				<xs:annotation>
					<xs:documentation>The time period that the content of a resource is
						about</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="sizePerTimeCoverage" type="ms:sizeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides information on size per time period represented in
						the resource</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<inline-type>tabular</inline-type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="geographicCoverageInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on geographic classification of the
				resource</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="geographicCoverage">
				<xs:annotation>
					<xs:documentation>The geographic region that the content of a resource is about;
						for countries, recommended use of ISO-3166</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="100"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="sizePerGeographicCoverage" type="ms:sizeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides information on size per geographically distinct
						section of the resource</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<inline-type>tabular</inline-type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="lingualityInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the number of languages of the resource part and
				of the way they are combined to each other</xs:documentation>
			<xs:appinfo>
				<render-short>{lingualityType}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="lingualityType">
				<xs:annotation>
					<xs:documentation>Indicates whether the resource includes one, two or more
						languages</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<label>Linguality type</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="monolingual"/>
						<xs:enumeration value="bilingual"/>
						<xs:enumeration value="multilingual"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="multilingualityType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Indicates whether the corpus is parallel, comparable or
						mixed</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<label>Multilinguality type</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="30"/>
						<xs:enumeration value="parallel"/>
						<xs:enumeration value="comparable"/>
						<xs:enumeration value="multilingualSingleText">
							<xs:annotation>
								<xs:documentation>for single texts that contain multiple language
									excerpts; e.g. european parliament
									discussions</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="multilingualityTypeDetails" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides further information on multilinguality of a resource
						in free text</xs:documentation>
					<xs:appinfo>
						<relation>one-to-many</relation>
						<label>Multilinguality type details</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="512"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<!--
			<xs:element ref="ms:modalityType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>information on modality</xs:documentation>
					<xs:appinfo>
						<relation>one-to-many</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
            -->
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="languageVarietyInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on language varieties occurred in the resource
				(e.g. dialects)</xs:documentation>
			<xs:appinfo>
				<render-short>{languageVarietyName} ({languageVarietyType})</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="languageVarietyType">
				<xs:annotation>
					<xs:documentation>Specifies the type of the language variety that occurs in the
						resource or is supported by a tool/service</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<label>Language variety type</label>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20"/>
						<xs:enumeration value="dialect"/>
						<xs:enumeration value="jargon"/>
						<xs:enumeration value="other"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element ref="ms:languageVarietyName">
				<xs:annotation>
					<xs:documentation>The name of the language variety that occurs in the resource
						or is supported by a tool/service</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="sizePerLanguageVariety" type="ms:sizeInfoType" minOccurs="0"
				maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Provides information on the size per language variety
						component</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<inline-type>tabular</inline-type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
