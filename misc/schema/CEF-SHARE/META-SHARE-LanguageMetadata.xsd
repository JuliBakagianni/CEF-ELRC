		<xs:annotation>
			<xs:documentation>Groups information on geographic classification of the
				resource</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="lingualityInfo">
		<xs:annotation>
			<xs:documentation>Groups information on the number of languages of the resource part and
				of the way they are combined to each other</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexType>
			<xs:complexContent>
				<xs:extension base="ms:lingualityInfoType">
					<xs:assert
						test="if (ms:lingualityType = 'monolingual') then (count(ms:multilingualityType) = 0  and count(ms:multilingualityTypeDetails) = 0) else true()"
						xerces:message="Monolingual linguality type cannot have either multilingualityType or multilingualityTypeDetails"
					/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="languageInfo" type="ms:languageInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the languages represented in the
				resource</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="languageVarietyInfo" type="ms:languageVarietyInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on language varieties occurred in the resource
				(e.g. dialects)</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
	<xs:element name="languageVarietyName">
		<xs:annotation>
			<xs:documentation>The name of the language variety that occurs in the resource or is
				supported by a tool/service</xs:documentation>
		</xs:annotation>
		<xs:simpleType>
			<xs:restriction base="xs:string">
				<xs:maxLength value="100"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
