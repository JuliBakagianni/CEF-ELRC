	<xs:complexType name="languageInfoType">
		<xs:annotation>
			<xs:documentation>Groups information on the languages represented in the resource </xs:documentation>
			<xs:appinfo>
				<render-short>{languageName} {languageVarietyInfo}</render-short>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="ms:languageId">
				<xs:annotation>
					<xs:documentation>The identifier of the language that is included in the
						resource or supported by the tool/service; an autocompletion mechanism with
						values from the ISO 639 is provided in the editor, but the values can be
						subsequently edited for further specification (according to the IETF BCP47
						guidelines)</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ms:languageName">
				<xs:annotation>
					<xs:documentation>A human understandable name of the language that is used in
						the resource or supported by the tool/service; an autocompletion mechanism
						with values from the ISO 639 is provided in the editor, but the values can
						be subsequently edited for further specification (according to the IETF
						BCP47 guidelines)</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="sizePerLanguage" type="ms:sizeInfoType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Provides information on the size per language
						component</xs:documentation>
					<xs:appinfo>
						<relation>one-to-one</relation>
						<inline-type>tabular</inline-type>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element ref="ms:languageVarietyInfo" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Groups information on language varieties occurred in the
						resource (e.g. dialects)</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<!-- defintion of elements -->
	<xs:element name="characterEncodingInfo" type="ms:characterEncodingInfoType">
		<xs:annotation>
			<xs:documentation>Groups together information on character encoding of the
				resource</xs:documentation>
			<xs:appinfo>
				<relation>one-to-one</relation>
			</xs:appinfo>
		</xs:annotation>
	</xs:element>
